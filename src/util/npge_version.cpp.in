#include <string>

#include "npge_version.hpp"

namespace npge {

void init_npge_version(lua_State* L) {
    std::string code = "npge = {};";
    code += "npge.VERSION = '@VERSION@'";
    luaL_dostring(L, code.c_str());
}

}

