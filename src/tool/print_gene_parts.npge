#! ${NPGE}

pipe PrintPartitionPipe {
    name "Print overlaps between two block sets as table";
    add AddBlocks target=pangenome prefix|pangenome-;
    add SequencesFromOther target=genes other=pangenome;
    add AddBlocks target=genes prefix|genes-;
    add SequencesFromOther target=groups other=pangenome;
    add AddBlocks target=groups prefix|groups-;
    add LinkEqualFragments target=groups other=genes;
    add PrintPartition target=groups other=pangenome;
};

run PrintPartitionPipe;

