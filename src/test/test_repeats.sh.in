${PROJECT_BINARY_DIR}/src/test/rand_seq 100000 > test.fasta &&
sleep 1
${PROJECT_BINARY_DIR}/src/test/rand_seq 100000 >> test.fasta &&
${PROJECT_BINARY_DIR}/src/find_anchors test.fasta 15 2 > repeats.txt &&
sed ':a;N;$!ba;s/\n//g' test.fasta > for_grep.txt &&
rev test.fasta | tac | tr atgc tacg | sed ':a;N;$!ba;s/\n//g' > complement.txt
for i in `cat repeats.txt`; do
    if [ `grep -o $i for_grep.txt complement.txt | wc -l` -lt 2 ]; then
        exit 1
    fi
done

